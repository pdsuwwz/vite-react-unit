    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "engines": {
    "node": ">=14.18.0"
  },
  "keywords": [
    "react",
    "vite",
    "jest",
    "typescript",
    "sample",
    "unit",
    "unit-testing"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdsuwwz/vite-react-unit"
  },
  "bugs": {
    "url": "https://github.com/pdsuwwz/vite-react-unit/issues"
  },
  "dependencies": {
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@sucrase/jest-plugin": "^2.2.1",
