{
  "name": "vite-react-unit",
  "description": "Unit Testing for React 17.x + TS",
  "version": "0.0.1",
  "author": "Wisdom <pdsu.wwz@foxmail.com>",
  "license": "MIT",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc && vite build",
    "preview": "vite preview --host",
