    "@vitejs/plugin-react-refresh": "^1.3.6",
    "babel-jest": "29.0.3",
    "babel-plugin-transform-vite-meta-env": "^1.0.3",
    "eslint": "^8.14.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.5.0",
    "jest": "29.0.3",
    "jest-environment-jsdom": "29.0.3",
    "jest-transform-stub": "^2.0.0",
